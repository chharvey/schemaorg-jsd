{
  "$schema": "https://chharvey.github.io/schemaorg-jsd/meta/type.jsd#",
  "$id": "https://chharvey.github.io/schemaorg-jsd/schema/Rating.jsd",
  "title": "http://schema.org/Rating",
  "description": "A rating is an evaluation on a numeric scale, such as 1 to 5 stars.",
  "allOf": [
    { "$ref": "Intangible.jsd" },
    {
      "properties": {
        "bestRating": {
          "$id": "#bestRating",
          "title": "http://schema.org/bestRating",
          "description": "The highest value allowed in this rating system. If bestRating is omitted, 5 is assumed.",
          "anyOf": [
            { "type": ["number", "string"] }
          ]
        },
        "ratingValue": {
          "$id": "#ratingValue",
          "title": "http://schema.org/ratingValue",
          "description": "The rating for the content.",
          "anyOf": [
            { "type": ["number", "string"] }
          ]
        },
        "worstRating": {
          "$id": "#worstRating",
          "title": "http://schema.org/worstRating",
          "description": "The lowest value allowed in this rating system. If worstRating is omitted, 1 is assumed.",
          "anyOf": [
            { "type": ["number", "string"] }
          ]
        }
      }
    }
  ]
}
