{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "https://chharvey.github.io/schemaorg-jsd/schema/logo.prop.jsd",
  "title": "http://schema.org/logo",
  "description": "An associated logo.",
  "anyOf": [
    {
      "$id": "#logo-type",
      "anyOf": [
        { "$ref": "URL.jsd" },
        { "$ref": "ImageObject.jsd" }
      ]
    },
    {
      "type": "array",
      "items": { "$ref": "#logo-type" }
    }
  ]
}
